  const [username, setUsername] = useState("");
  const [passwordUsuario, setPasswordUsuario] = useState("");

  const usuarioLogueado = props.clientes.find(
      (cliente) => cliente.username === username
    );

  if (typeof usuarioLogueado === 'undefined'){
     const usuarioAdminLogueado = props.peluquerias.find(
      (peluquerias) => peluquerias.username === username
    );
  }